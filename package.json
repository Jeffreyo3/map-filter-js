{
  "name": "map-filter-js",
  "author": "jeffreyo3",
  "version": "1.1.1",
  "license": "MIT",
  "description": "A utility that will filter and possibly transform in a single iteration of an array",
  "type": "module",
  "devDependencies": {
    "@types/jest": "30.0",
    "@types/node": "^20.1.0",
    "jest": "^30.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "overrides": {
    "js-yaml": "4.1.1"
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "scripts": {
    "build": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "test": "jest --no-cache --coverage --testPathPatterns=src/",
    "test:watch": "jest --watchAll --testPathPatterns=src/",
    "test:bench": "npm run build && npm run build:cjs && jest --no-cache --testPathPatterns=benchmarks/"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jeffreyo3/map-filter-js.git"
  },
  "bugs": {
    "url": "https://github.com/Jeffreyo3/map-filter-js/issues"
  },
  "homepage": "https://github.com/Jeffreyo3/map-filter-js#readme"
}
